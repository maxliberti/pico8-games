-- draw --

function draw_game()
	cls()
	map()
	draw_yungadd()
	draw_bands(yungadd)
	for guap in all(guaps) do guap:draw() end
	for tri in all(triangles) do tri:draw()	end
	for heart in all(hearts) do heart:draw() end
end

function draw_game_over()
	cls()
	map(16,0)
	print("game over",45,55,7)
	print("press x to continue",25,62,7)
end
