-- yung add  --

function yungadd_update(o)
	-- movement
	
	if btn(➡️) then o.x+=o.speed end
 if btn(⬅️) then o.x-=o.speed end
 if btn(⬆️) then o.y-=o.speed end
 if btn(⬇️) then o.y+=o.speed end
 
 -- stay in bounds
 if o.x<rightedge then
 	o.x=rightedge
 end
 if o.x>leftedge then
 	o.x=leftedge
 end
 if o.y<topedge then
 	o.y=topedge
 end
 if o.y>bottomedge then
 	o.y=bottomedge
 end
 
 --what happens if he gets hit
 on_hit(o)
  
end

function draw_yungadd()
	spr(1,yungadd.x,yungadd.y,2,2)
	on_hit(yungadd)
end


