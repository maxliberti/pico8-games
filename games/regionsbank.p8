-- triangles --
function spawn_triangles(spawnrate)
	if framecount % spawnrate == 0 then
		add_triangle()
	end
end

function add_triangle()

	--handle coord generation
	coords={0,20,40,60,80,100,120}
	edgecoords={0,120}
	
	xcoord=rnd(coords)
	
	if xcoord==0 or xcoord==120 then
		ycoord=rnd(coords)
	else
		ycoord=rnd(edgecoords)
	end
	
	--detect spawn location
	spawntypes = {"left","right","top","bottom"}
	if xcoord==0 then
		spawntype=1
	elseif xcoord==120 then
		spawntype=2
	elseif ycoord==0 then
		spawntype=3
	elseif ycoord==120 then
		spawntype=4
	end
		
	--init and add triangle
	triangle={
		x=xcoord,
		y=ycoord,
		spr=4,
		spawntype=spawntypes[spawntype],
		movestate=rnd(tristates),
		draw=draw_triangle
	}
	
	add(triangles,triangle)
end

function draw_triangle(tri)
	spr(tri.spr,tri.x,tri.y)
end

function determine_triangle_movement(rndstate)

	for tri in all(triangles) do
		if tri.spawntype=="left" then
			if tri.movestate=="straight" then
				tri.x+=trispeed
			elseif tri.movestate=="diag1" then
				tri.x+=trispeed
				tri.y-=trispeed
			elseif tri.movestate=="diag2" then
				tri.x+=trispeed
				tri.y+=trispeed
			end
		elseif tri.spawntype=="right" then
			if tri.movestate=="straight" then
				tri.x-=trispeed
			elseif tri.movestate=="diag1" then
				tri.x-=trispeed
				tri.y-=trispeed
			elseif tri.movestate=="diag2" then
				tri.x-=trispeed
				tri.y+=trispeed
			end
		elseif tri.spawntype=="top" then
			if tri.movestate=="straight" then
				tri.y+=trispeed
			elseif tri.movestate=="diag1" then
				tri.x+=trispeed
				tri.y+=trispeed
			elseif tri.movestate=="diag2" then
				tri.x-=trispeed
				tri.y+=trispeed
			end
		elseif tri.spawntype=="bottom" then
			if tri.movestate=="straight" then
				tri.y-=trispeed
			elseif tri.movestate=="diag1" then
				tri.x+=trispeed
				tri.y-=trispeed
			elseif tri.movestate=="diag2" then
				tri.x-=trispeed
				tri.y-=trispeed
			end
		end
	end
end
