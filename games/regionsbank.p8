-- update --
function update_game()
	if not stat(57) then
		music(0)
	end
	framecount+=1
	yungadd_update(yungadd)
	spawn_triangles(trispawnrate)
	spawn_guaps()
	determine_triangle_movement(rndstate) 
	tri_collision_scan(yungadd,triangles)
	guap_collision_scan(yungadd,guaps)
end

function game_over()
	music(-1)
	if btn(❎) then
		_init()
	end
end
