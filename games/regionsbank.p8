-- guap --

function spawn_guaps()
	if #guaps == 0 then
		add_guaps()
	end
end

function add_guaps()
	local leftside=120
	local rightside=0
	local topside=0
	local bottomside=120
	
	guap={
		x=flr(rnd(leftside-rightside+1)),
		y=flr(rnd(bottomside-topside+1)),
		spr=22,
		draw=draw_guap
	}
	
	add(guaps,guap)
end

function draw_guap(guap)
	spr(guap.spr,guap.x,guap.y)
end

