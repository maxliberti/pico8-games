-- ui --

-- money
function draw_bands(o)
	print("bands:",1,1,0)
	print(o.wealth,26,1,0)
	spr(11,1,5,2,2)
end

-- hearts
function init_hearts()
	offset=0
	for i=1,3 do 
		heart={
			x=heartx+offset,
			y=hearty,
			spr=19,
			draw=draw_heart
		}
		
		add(hearts,heart)
		
		offset+=8
	end
end

function on_hit(o)
	if o.state!="threehits" then				
			if o.state=="onehit" then
				hearts[3].spr=20
			elseif o.state=="twohits" then
				hearts[3].spr=20
				hearts[2].spr=20
			end
	else
	-- game over scenario
		hearts[3].spr=20
		hearts[2].spr=20
		hearts[1].spr=20
	end
end

function draw_heart(heart)
		spr(heart.spr,heart.x,heart.y)
end
