-- collision --
function col(yung,thing)
	local add_left=yung.x
	local add_top=yung.y
	local add_right=yung.x+15
	local add_bottom=yung.y+15
	
	local thing_left=thing.x
	local thing_top=thing.y
	local thing_right=thing.x+7
	local thing_bottom=thing.y+7
	
	if add_top>thing_bottom then return false end
	if thing_top>add_bottom then return false end
	if add_left>thing_right then return false end
	if thing_left>add_right then return false end
	
	return true
end

function tri_collision_scan(o,triangles)
	for tri in all(triangles) do
		if col(o,tri) then
			sfx(0)
			yungstate+=1
			o.state=yungstates[yungstate]
			del(triangles,tri)
		end
	end
end

function guap_collision_scan(o,guaps)
	for guap in all(guaps) do
		if col(o,guap) then
			sfx(1)
			o.wealth+=1
			del(guaps,guap)
		end
	end
end
