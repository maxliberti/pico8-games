-- main --
function _init()
	-- misc
	palt(o,false)
	palt(1,true)
	
	leftedge=112
	rightedge=0
	topedge=0
	bottomedge=112
	
	framecount=0
	bands=0
	
	-- yung add init
	yungstates={"nohits","onehit","twohits","threehits"}
	yungstate=1
	
	yungadd={
	x=63,
	y=63,
	speed=2,
	health=3,
	wealth=0,
	state=yungstates[yungstate],
	}
	
	-- triangle stuff
	triangles={}
	trng=100
	tristates={"straight","diag1","diag2"}
	trispeed=1
	trispawnrate=30
	
	-- health stuff
	heartx=104
	hearty=0
	hearts={}
	init_hearts()
	
	-- money stuff
	guaps={}
	
	
end

function _update()
	if yungadd.state!="threehits" then
		update_game()
	else
		game_over()
	end
end

function _draw()
	if yungadd.state!="threehits" then
	 draw_game()
	else
		draw_game_over()
	end
end

